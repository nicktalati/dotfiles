{
  "manifest_version": 3,
  "name": "Google Voice Color Inverter",
  "version": "1.0",
  "description": "Invert all colors on voice.google.com.",
  "content_scripts": [
    {
      "matches": ["*://voice.google.com/*"],
      "css": ["invert.css"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
