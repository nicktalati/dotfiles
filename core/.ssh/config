Include config.d/*

Host github.com
	User git
	IdentityFile ~/.ssh/id_rsa_personal

Host personal
	HostName ssh.github.com
    Port 443
	User git
	IdentityFile ~/.ssh/id_rsa_personal

Host work
	HostName ssh.github.com
    Port 443
	User git
	IdentityFile ~/.ssh/id_rsa_work_github

Host work-gitlab
	HostName gitlab.com
	User git
	IdentityFile ~/.ssh/id_rsa_work_gitlab
